{
  "name": "ironguard-mercs",
  "tree": {
    "$className": "DataModel",
    "Workspace": {
      "$className": "Workspace"
    },
    "ReplicatedStorage": {
      "TS": {
        "$path": "out/shared"
      },
      "rbxts_include": {
        "$className": "Folder",
        "$path": "include",
        "node_modules": {
          "$className": "Folder",
          "@rbxts": {
            "$path": "node_modules/@rbxts"
          },
          "@rbxts-js": {
            "$path": "node_modules/@rbxts-js"
          },
          "@flamework": {
            "$path": "node_modules/@flamework"
          }
        }
      }
    },
    "ServerScriptService": {
      "TS": {
        "$path": "out/server"
      }
    },
    "ServerStorage": {
      "EnemyRigs": {
        "$path": "assets/enemy-rigs"
      },
      "Weapons": {
        "$path": "assets/weapons"
      },
      "Tiles": {
        "$path": "assets/tiles"
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "TS": {
          "$path": "out/client"
        }
      }
    },
    "Lighting": {
      "$className": "Lighting",
      "$properties": {
        "Technology": "Future",
        "GlobalShadows": true,
        "OutdoorAmbient": [
          0.2,
          0.2,
          0.3
        ],
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 1.5,
        "EnvironmentDiffuseScale": 0.4,
        "EnvironmentSpecularScale": 0.4
      },
      "Atmosphere": {
        "$className": "Atmosphere",
        "$properties": {
          "Density": 0.45,
          "Offset": 0,
          "Color": [
            0.15,
            0.2,
            0.25
          ],
          "Decay": [
            0.2,
            0.15,
            0.1
          ],
          "Glare": 0,
          "Haze": 2
        }
      }
    }
  }
}